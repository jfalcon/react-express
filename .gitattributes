# auto detect text files
* text=auto

# force all text file types to have Unix line endings
*.* text eol=lf

# files that are truly binary and should not be modified
*.ai binary
*.bmp binary
*.gif binary
*.jpg binary
*.jpeg binary
*.pdf binary
*.png binary
*.psd binary
*.tif binary
*.tiff binary

*.data binary
*.eof binary
*.flv binary
*.mov binary
*.mp3 binary
*.mp4 binary
*.ogg binary
*.swf binary
*.t3d binary
*.t3x binary

*.otf binary
*.eot binary
*.ttf binary
*.woff binary
*.woff2 binary
