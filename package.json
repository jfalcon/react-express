{
  "author": "Jeremy Falcon",
  "description": "Viking Cruises",
  "homepage": "https://www.vikingcruises.com/",
  "name": "viking-cruises",
  "repository": "git",
  "private": true,
  "version": "0.0.1",
  "config": {
    "charset": "utf-8",
    "database": "mongodb://localhost/mydb",
    "mockDelay": 1000,
    "defaults": {
      "filename": "index"
    },
    "directories": {
      "images": "images",
      "modules": "node_modules",
      "output": ".dist",
      "source": {
        "client": "client",
        "server": "server"
      }
    },
    "ports": {
      "client": 8080,
      "server": 3000
    }
  },
  "keywords": [
    "viking",
    "cruises"
  ],
  "pre-commit": [
    "lint",
    "test"
  ],
  "scripts": {
    "prebuild": "npm-run-all --silent message clean lint test",
    "build": "cross-env NODE_ENV=production babel-node ./tools/build.js",
    "clean": "rimraf ./.dist",
    "client": "cross-env NODE_ENV=production babel-node ./tools/client.js",
    "client:watch": "babel-node ./tools/client.js",
    "lint": "esw client locale server tools",
    "lint:watch": "npm run lint -- --watch",
    "message": "babel-node ./tools/message.js",
    "serve": "npm-run-all --silent --parallel server client",
    "server": "cross-env NODE_ENV=production babel-node ./server/index.js",
    "server:watch": "babel-node ./server/index.js",
    "prestart": "npm-run-all --silent message",
    "start": "babel-node ./tools/build.js",
    "poststart": "npm-run-all --silent --parallel lint:watch server:watch client:watch",
    "test": "mocha --reporter progress ./tools/test.js \"./tests/**/*.js\""
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "body-parser": "1.18.2",
    "compression": "1.7.1",
    "cookie-session": "2.0.0-beta.3",
    "express": "4.16.2",
    "flexibility": "2.0.1",
    "helmet": "3.11.0",
    "history": "4.7.2",
    "intl": "1.2.5",
    "isomorphic-fetch": "2.2.1",
    "mongoose": "5.0.3",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "3.1.3",
    "react-intl": "2.4.0",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "3.7.2",
    "redux-saga": "0.16.0",
    "resource-router-middleware": "0.7.0",
    "sprintf-js": "1.1.1",
    "uuid": "3.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "7.2.5",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-loader": "7.1.2",
    "babel-plugin-conditional-compilation": "0.0.1",
    "babel-plugin-module-resolver": "3.0.0",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.26.0",
    "bootstrap": "4.0.0",
    "bootstrap.native": "2.0.21",
    "colors": "1.1.2",
    "cross-env": "5.1.3",
    "css-loader": "0.28.9",
    "eslint": "4.17.0",
    "eslint-import-resolver-babel-module": "4.0.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-react": "7.6.1",
    "eslint-watch": "3.1.3",
    "eventsource-polyfill": "0.9.6",
    "expect": "22.1.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "2.30.1",
    "jsdom": "11.6.2",
    "mocha": "5.0.0",
    "node-sass": "4.7.2",
    "npm-run-all": "4.1.2",
    "open": "0.0.5",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "2.1.0",
    "pre-commit": "1.2.2",
    "progress-bar-webpack-plugin": "1.10.0",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-logger": "3.0.6",
    "resolve-url-loader": "2.2.1",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.20.1",
    "stylelint": "8.4.0",
    "stylelint-webpack-plugin": "0.10.1",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-middleware": "2.0.4",
    "webpack-hot-middleware": "2.21.0"
  }
}
